<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="-13dp">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="157dp"
        android:layout_height="156dp"
        android:contentDescription="Logo"
        app:layout_constraintBottom_toBottomOf=""
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf=""
        app:srcCompat="@drawable/logo"
        tools:ignore="HardcodedText,MissingConstraints" />

    <Button
        android:id="@+id/button"
        android:layout_width="267dp"
        android:layout_height="50dp"
        android:radius="15dp"
        android:text="Log In"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteX="73dp"
        tools:layout_editor_absoluteY="636dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:focusable="true"
        android:text="Already have an account?"
        android:textColor="#913DA6"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="686dp"
        tools:ignore="HardcodedText,MissingConstraints" />

    <EditText
        android:id="@+id/TextPW"
        android:layout_width="130dp"
        android:layout_height="50dp"
        android:background="#E6E0E9"
        android:ems="10"
        android:hint="Last name"
        android:inputType="textPassword"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="210dp"
        tools:layout_editor_absoluteY="205dp"
        tools:ignore="HardcodedText" />

    <EditText
        android:id="@+id/dobDate"
        android:layout_width="270dp"
        android:layout_height="50dp"
        android:background="#E6E0E9"
        android:ems="10"
        android:hint="Email Address:"
        android:inputType="date"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="324dp"
        tools:ignore="HardcodedText,MissingConstraints" />

    <EditText
        android:id="@+id/username"
        android:layout_width="130dp"
        android:layout_height="50dp"
        android:background="#E6E0E9"
        android:ems="10"
        android:hint="First name"
        android:inputType="text"
        android:singleLine="true"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="205dp"
        tools:ignore="HardcodedText,MissingConstraints" />

    <EditText
        android:id="@+id/editTextDate2"
        android:layout_width="270dp"
        android:layout_height="50dp"
        android:background="#E6E0E9"
        android:ems="10"
        android:hint="DOB:"
        android:inputType="date"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="263dp"
        tools:ignore="HardcodedText" />

    <EditText
        android:id="@+id/editTextPhone"
        android:layout_width="270dp"
        android:layout_height="50dp"
        android:background="#E6E0E9"
        android:ems="10"
        android:hint="Phone Number:"
        android:inputType="phone"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="383dp"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Register here!"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="130dp"
        tools:layout_editor_absoluteY="163dp"
        tools:ignore="HardcodedText"
        app:layout_constraintLeft_toLeftOf=""
        app:layout_constraintTop_toTopOf=""
        app:layout_constraintRight_toRightOf=""
        app:layout_constraintBottom_toBottomOf=""/>

    <EditText
        android:id="@+id/enterPW"
        android:layout_width="270dp"
        android:layout_height="50dp"
        android:background="#E6E0E9"
        android:ems="10"
        android:hint="Enter password:"
        android:inputType="textPassword"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="504dp"
        tools:ignore="HardcodedText" />

    <EditText
        android:id="@+id/cfmPW"
        android:layout_width="270dp"
        android:layout_height="50dp"
        android:background="#E6E0E9"
        android:ems="10"
        android:hint="Confirm password:"
        android:inputType="textPassword"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="568dp"
        tools:ignore="HardcodedText" />

    <RadioButton
        android:id="@+id/rbFemale"
        android:layout_width="130dp"
        android:layout_height="50dp"
        android:text="Female"
        android:textSize="14sp"
        android:background="#E6E0E9"
        tools:layout_editor_absoluteX="211dp"
        tools:layout_editor_absoluteY="442dp"
        tools:ignore="HardcodedText"
        app:layout_constraintLeft_toLeftOf=""
        app:layout_constraintTop_toTopOf=""
        app:layout_constraintRight_toRightOf=""
        app:layout_constraintBottom_toBottomOf=""/>

    <RadioButton
        android:id="@+id/rbMale"
        android:layout_width="130dp"
        android:layout_height="50dp"
        android:background="#E6E0E9"
        android:checked="true"
        android:text="Male"
        android:textSize="14sp"
        tools:ignore="HardcodedText"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="442dp" />

</androidx.constraintlayout.widget.ConstraintLayout>