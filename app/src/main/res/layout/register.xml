<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteX="2dp"
    tools:layout_editor_absoluteY="0dp">
    <RelativeLayout
        android:id="@+id/relativeLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#F4F1E1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="16dp">

            <ImageView
                android:id="@+id/logo"
                android:layout_width="114dp"
                android:layout_height="113dp"
                android:layout_centerInParent="true"
                android:src="@drawable/dietdietmusttry_logo" />

        </RelativeLayout>
    </RelativeLayout>

    <Button
        android:id="@+id/button"
        android:layout_width="267dp"
        android:layout_height="50dp"
        android:layout_marginStart="73dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="71dp"
        android:radius="15dp"
        android:text="Log In"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cfmPW"
        tools:ignore="HardcodedText,MissingConstraints" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:focusable="true"
        android:text="Already have an account?"
        android:textColor="#913DA6"
        android:textSize="12sp"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="686dp" />

    <EditText
        android:id="@+id/TextPW"
        android:layout_width="130dp"
        android:layout_height="50dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="8dp"
        android:background="#E6E0E9"
        android:ems="10"
        android:hint="Last name"
        android:inputType="textPassword"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@+id/username"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:ignore="HardcodedText" />

    <EditText
        android:id="@+id/dobDate"
        android:layout_width="270dp"
        android:layout_height="50dp"
        android:layout_marginTop="12dp"
        android:background="#E6E0E9"
        android:ems="10"
        android:hint="Email Address:"
        android:inputType="date"
        android:textSize="14sp"
        app:layout_constraintTop_toBottomOf="@+id/editTextDate2"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteX="70dp" />

    <EditText
        android:id="@+id/username"
        android:layout_width="130dp"
        android:layout_height="50dp"
        android:layout_marginStart="70dp"
        android:layout_marginTop="8dp"
        android:background="#E6E0E9"
        android:ems="10"
        android:hint="First name"
        android:inputType="text"
        android:singleLine="true"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@+id/TextPW"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:ignore="HardcodedText,MissingConstraints" />

    <EditText
        android:id="@+id/editTextDate2"
        android:layout_width="270dp"
        android:layout_height="50dp"
        android:background="#E6E0E9"
        android:ems="10"
        android:hint="DOB:"
        android:inputType="date"
        android:textSize="14sp"
        tools:ignore="HardcodedText"
        tools:layout_editor_absoluteX="70dp"
        tools:layout_editor_absoluteY="263dp" />

    <EditText
        android:id="@+id/editTextPhone"
        android:layout_width="270dp"
        android:layout_height="50dp"
        android:layout_marginTop="16dp"
        android:background="#E6E0E9"
        android:ems="10"
        android:hint="Phone Number:"
        android:inputType="phone"
        android:textSize="14sp"
        app:layout_constraintTop_toBottomOf="@+id/dobDate"
        tools:ignore="HardcodedText"
        tools:layout_editor_absoluteX="70dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Register here!"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf=""
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf=""
        tools:ignore="HardcodedText"
        tools:layout_editor_absoluteX="130dp"
        tools:layout_editor_absoluteY="163dp" />

    <EditText
        android:id="@+id/enterPW"
        android:layout_width="270dp"
        android:layout_height="50dp"
        android:layout_marginTop="12dp"
        android:background="#E6E0E9"
        android:ems="10"
        android:hint="Enter password:"
        android:inputType="textPassword"
        android:textSize="14sp"
        app:layout_constraintTop_toBottomOf="@+id/rbFemale"
        tools:ignore="HardcodedText"
        tools:layout_editor_absoluteX="70dp" />

    <EditText
        android:id="@+id/cfmPW"
        android:layout_width="270dp"
        android:layout_height="50dp"
        android:layout_marginTop="12dp"
        android:background="#E6E0E9"
        android:ems="10"
        android:hint="Confirm password:"
        android:inputType="textPassword"
        android:textSize="14sp"
        app:layout_constraintTop_toBottomOf="@+id/enterPW"
        tools:ignore="HardcodedText"
        tools:layout_editor_absoluteX="71dp" />

    <RadioButton
        android:id="@+id/rbMale"
        android:layout_width="130dp"
        android:layout_height="50dp"
        android:layout_marginTop="12dp"
        android:background="#E6E0E9"
        android:checked="true"
        android:text="Male"
        android:textSize="14sp"
        app:layout_constraintTop_toBottomOf="@+id/editTextPhone"
        tools:ignore="HardcodedText"
        tools:layout_editor_absoluteX="70dp" />

    <RadioButton
        android:id="@+id/rbFemale"
        android:layout_width="130dp"
        android:layout_height="50dp"
        android:background="#E6E0E9"
        android:text="Female"
        android:textSize="14sp"
        tools:ignore="HardcodedText"
        tools:layout_editor_absoluteX="210dp"
        tools:layout_editor_absoluteY="454dp" />

</androidx.constraintlayout.widget.ConstraintLayout>