<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/button"
        android:layout_width="85dp"
        android:layout_height="50dp"
        android:backgroundTint="#F03737"
        android:radius="15dp"
        android:text="Log Out"
        android:textSize="10sp"
        tools:layout_editor_absoluteX="310dp"
        tools:layout_editor_absoluteY="30dp"
        tools:ignore="HardcodedText,SmallSp"
        />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="113dp"
        android:layout_height="113dp"
        app:srcCompat="@drawable/diet_and_nutrition__1_"
        tools:layout_editor_absoluteX="-1dp"
        tools:layout_editor_absoluteY="1dp" />

    <TextView
        android:id="@+id/textview1"
        android:layout_width="178dp"
        android:layout_height="24dp"
        android:text="Welcome back, Weiss!"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="116dp"
        tools:layout_editor_absoluteY="73dp"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textview2"
        android:layout_width="98dp"
        android:layout_height="26dp"
        android:text="Home Page"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="156dp"
        tools:layout_editor_absoluteY="30dp"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/titletable"
        android:layout_width="155dp"
        android:layout_height="26dp"
        android:text="Summary of Users"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:ignore="HardcodedText"
        tools:layout_editor_absoluteX="128dp"
        tools:layout_editor_absoluteY="112dp" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:layout_width="409dp"
        android:layout_height="84dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="591dp" />

    <TableLayout
        android:layout_width="414dp"
        android:layout_height="153dp"
        android:stretchColumns="1"
        tools:layout_editor_absoluteX="-3dp"
        tools:layout_editor_absoluteY="138dp">

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#000000" />

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:layout_gravity="center_vertical"
                android:padding="8dp"
                android:text="Number of Registered users:"
                tools:ignore="HardcodedText" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:padding="8dp"
                android:text="80"
                tools:ignore="HardcodedText" />

        </TableRow>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#000000" />

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:layout_gravity="center_vertical"
                android:padding="8dp"
                android:text="Number of Nutritionists:"
                tools:ignore="HardcodedText" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:padding="8dp"
                android:text="20"
                tools:ignore="HardcodedText" />

        </TableRow>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#000000" />

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:layout_gravity="center_vertical"
                android:padding="8dp"
                android:text="Number of Admins:"
                tools:ignore="HardcodedText" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:padding="8dp"
                android:text="10"
                tools:ignore="HardcodedText" />

        </TableRow>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#000000" />

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:layout_gravity="center_vertical"
                android:padding="8dp"
                android:text="Total Number of Users:"
                tools:ignore="HardcodedText" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:padding="8dp"
                android:text="110"
                tools:ignore="HardcodedText" />

        </TableRow>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#000000" />

    </TableLayout>

    <Button
        android:id="@+id/addAdminButton"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:backgroundTint="#9C27B0"
        android:radius="15dp"
        android:text="View all accounts"
        android:textSize="10sp"
        tools:ignore="HardcodedText,MissingConstraints,SmallSp"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="300dp" />

    <Button
        android:id="@+id/addFAQbutton"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:backgroundTint="#4CAF50"
        android:radius="15dp"
        android:text="Add an FAQ"
        android:textSize="10sp"
        tools:ignore="HardcodedText,MissingConstraints,SmallSp"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="460dp" />

    <Button
        android:id="@+id/backButton"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:backgroundTint="#009688"
        android:radius="15dp"
        android:text="Add an admin"
        android:textSize="10sp"
        tools:ignore="HardcodedText,MissingConstraints,SmallSp"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="350dp" />

    <TableLayout
        android:layout_width="415dp"
        android:layout_height="48dp"
        android:stretchColumns="1"
        tools:layout_editor_absoluteX="-2dp"
        tools:layout_editor_absoluteY="412dp">

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#000000" />

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:layout_gravity="center_vertical"
                android:padding="8dp"
                android:text="Number of FAQs:"
                tools:ignore="HardcodedText" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:padding="8dp"
                android:text="8"
                tools:ignore="HardcodedText" />

        </TableRow>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#000000" />
    </TableLayout>

</androidx.constraintlayout.widget.ConstraintLayout>